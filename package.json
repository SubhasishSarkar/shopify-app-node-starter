{
  "name": "shopify-app-node-starter",
  "version": "1.0.0",
  "description": "This is a starter project for creating Shopify Apps with a Neo4J database and ES6 Node.JS",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel server -d build/server --presets es2015,stage-1 && cp -r server/views/ build/server/views", //this is complex so it can run on heroku
    "build": "npm run clean && npm run build-server",
    "start": "npm run build && node ./build/server/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhillhouse92/shopify-app-node-starter.git"
  },
  "author": "Joseph Hillhouse",
  "bugs": {
    "url": "https://github.com/jhillhouse92/shopify-app-node-starter/issues"
  },
  "homepage": "https://github.com/jhillhouse92/shopify-app-node-starter#readme",
  "devDependencies": {
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "body-parser": "^1.15.0",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "node-uuid": "^1.4.7",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "seraph": "^0.14.2",
    "shopify-node-api": "^1.6.3"
  }
}
